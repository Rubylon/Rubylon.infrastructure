log_format json '{'
  '<%= @log_format.map { |k, v| %("#{k}": "#{v}") }.join(", '\n\t'") %>'
'}';
access_log  /var/log/nginx/access.json.log json;

log_format dependencyapi '$remote_addr - $remote_user [$time_local] '
                    '"$request" $status $body_bytes_sent '
                    '"$http_referer" "$http_user_agent" "$dependency_api_upstream"';
